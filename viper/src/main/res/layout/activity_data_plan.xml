<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">


    <android.support.constraint.ConstraintLayout
        android:id="@+id/layout_dataplan"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/tv_hint"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/wave_nav_header_image_border"
            android:paddingStart="@dimen/padding_10"
            android:paddingTop="@dimen/padding_5"
            android:paddingEnd="@dimen/padding_10"
            android:paddingBottom="@dimen/padding_5"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_16"
            android:visibility="gone"
            app:layout_constraintTop_toBottomOf="@+id/info_layout" />

        <android.support.constraint.ConstraintLayout
            android:id="@+id/mesh_user_layout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/margin_15"
            android:layout_marginTop="@dimen/margin_20"
            android:layout_marginEnd="@dimen/margin_15"
            android:background="@drawable/ractangular_light_yellow"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_hint">

            <TextView
                android:id="@+id/default_info"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/margin_10"
                android:layout_marginStart="@dimen/margin_10"
                android:layout_marginEnd="@dimen/margin_10"
                android:fontFamily="@font/opensans_regular"
                android:text="@string/mesh_user_info"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_16"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </android.support.constraint.ConstraintLayout>

        <android.support.constraint.ConstraintLayout
            android:id="@+id/internet_user_layout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/margin_15"
            android:layout_marginTop="@dimen/margin_20"
            android:layout_marginEnd="@dimen/margin_15"
            android:background="@drawable/ractangular_light_yellow"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_hint">

            <TextView
                android:id="@+id/default_internet_user_info"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/margin_10"
                android:layout_marginStart="@dimen/margin_10"
                android:layout_marginEnd="@dimen/margin_10"
                android:fontFamily="@font/opensans_regular"
                android:text="@string/internet_user_info"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_16"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </android.support.constraint.ConstraintLayout>


        <android.support.constraint.ConstraintLayout
            android:id="@+id/info_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/drawable_reg_page_shape"
            android:paddingBottom="@dimen/padding_20"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/status_bar">

            <ImageView
                android:id="@+id/imageView_back"
                android:layout_width="@dimen/width_40"
                android:layout_height="@dimen/height_40"
                android:layout_marginLeft="@dimen/margin_5"
                android:layout_marginTop="@dimen/margin_5"
                android:padding="@dimen/padding_10"
                android:src="@mipmap/ic_back_white"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/ic_wallet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/margin_10"
                android:padding="@dimen/padding_10"
                android:src="@mipmap/ic_wallet"
                app:layout_constraintBottom_toBottomOf="@+id/title"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/title" />

            <TextView
                android:id="@+id/currency"
                app:layout_constraintEnd_toStartOf="@+id/ic_wallet"
                app:layout_constraintTop_toTopOf="@+id/ic_wallet"
                app:layout_constraintBottom_toBottomOf="@+id/ic_wallet"
                android:textColor="@color/white"
                android:layout_marginEnd="@dimen/margin_10"
                android:text="@string/etc"
                android:visibility="gone"
                android:textSize="@dimen/text_size_20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <RadioGroup
                android:id="@+id/data_plan_type"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_20"
                app:layout_constraintTop_toBottomOf="@+id/imageView_back">

                <RadioButton
                    android:id="@+id/mesh_user"
                    style="@style/data_share_radio"
                    android:checked="@{dataLimitModel.initialRole == PreferencesHelper.MESH_USER}"
                    android:text="@string/mesh_user"
                    tools:ignore="UnusedAttribute" />

                <RadioButton
                    android:id="@+id/data_seller"
                    style="@style/data_share_radio"
                    android:layout_marginTop="@dimen/margin_10"
                    android:checked="@{dataLimitModel.initialRole == PreferencesHelper.DATA_SELLER}"
                    android:text="@string/data_seller"
                    tools:ignore="UnusedAttribute" />

                <RadioButton
                    android:id="@+id/data_buyer"
                    style="@style/data_share_radio"
                    android:layout_marginTop="@dimen/margin_10"
                    android:checked="@{dataLimitModel.initialRole == PreferencesHelper.DATA_BUYER}"
                    android:text="@string/data_buyer"
                    tools:ignore="UnusedAttribute" />

                <RadioButton
                    android:id="@+id/internet_user"
                    style="@style/data_share_radio"
                    android:layout_marginTop="@dimen/margin_10"
                    android:checked="@{dataLimitModel.initialRole == PreferencesHelper.INTERNET_USER}"
                    android:text="@string/internet_user"
                    tools:ignore="UnusedAttribute" />

            </RadioGroup>

            <TextView
                android:id="@+id/title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_15"
                android:gravity="center"
                android:text="@string/data_plan"
                android:textColor="@color/white"
                android:textSize="@dimen/text_size_22"
                app:layout_constraintBottom_toBottomOf="@+id/imageView_back"
                app:layout_constraintLeft_toRightOf="@+id/imageView_back"
                app:layout_constraintTop_toTopOf="@+id/imageView_back" />

        </android.support.constraint.ConstraintLayout>

        <android.support.constraint.ConstraintLayout
            android:id="@+id/data_sell_layout"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_hint">

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="8dp"
                android:fillViewport="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <android.support.constraint.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/share_internet_settings"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_25"
                        android:layout_marginTop="@dimen/margin_10"
                        android:layout_marginEnd="@dimen/margin_25"
                        android:fontFamily="@font/opensans_semibold"
                        android:text="@string/share_internet_settings"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_22"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/save_button"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/margin_20"
                        android:layout_marginEnd="@dimen/margin_15"
                        android:layout_marginRight="@dimen/margin_15"
                        android:background="@drawable/ractangular_green_small"
                        android:fontFamily="@font/opensans_semibold"
                        android:paddingLeft="@dimen/padding_25"
                        android:paddingTop="@dimen/padding_10"
                        android:paddingRight="@dimen/padding_25"
                        android:paddingBottom="@dimen/padding_10"
                        android:text="Save"
                        android:textAlignment="center"
                        android:textColor="@color/colorGradientPrimary"
                        android:textSize="@dimen/text_small_body_14"
                        android:visibility="visible"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/enable_disable_layout" />

                    <TextView
                        android:id="@+id/sell_price_info"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/margin_25"
                        android:fontFamily="@font/opensans_semibold"
                        android:gravity="center"
                        android:textColor="@color/dark_blue"
                        android:textSize="@dimen/text_size_18"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/range_divider_vertical" />

                    <TextView
                        android:id="@+id/fixed_network_info"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/margin_5"
                        android:fontFamily="@font/opensans_regular"
                        android:gravity="center"
                        android:text="@string/fixed_by_the_network"
                        android:textColor="@color/deep_grey"
                        android:textSize="@dimen/text_size_16"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/sell_price_info" />


                    <View
                        android:id="@+id/state_divider_vertical"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/width_1"
                        android:layout_marginStart="@dimen/margin_25"
                        android:layout_marginTop="@dimen/margin_10"
                        android:layout_marginEnd="@dimen/margin_25"
                        android:background="@color/grey_400"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/share_internet_settings" />

                    <View
                        android:id="@+id/range_divider_vertical"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/width_1"
                        android:layout_marginStart="@dimen/margin_25"
                        android:layout_marginTop="@dimen/margin_20"
                        android:layout_marginEnd="@dimen/margin_25"
                        android:background="@color/grey_400"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/save_button" />

                    <android.support.constraint.ConstraintLayout
                        android:id="@+id/enable_disable_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_25"
                        android:layout_marginTop="@dimen/margin_10"
                        android:layout_marginEnd="@dimen/margin_25"
                        android:alpha="0.5"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/state_divider_vertical">

                        <EditText
                            android:id="@+id/range"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/margin_55"
                            android:layout_marginLeft="@dimen/margin_55"
                            android:layout_marginTop="@dimen/margin_5"
                            android:background="@drawable/ractangular_round_white"
                            android:clickable="false"
                            android:digits="0123456789"
                            android:inputType="number"
                            android:maxLength="4"
                            android:maxLines="1"
                            android:minWidth="@dimen/width_50"
                            android:paddingLeft="@dimen/padding_10"
                            android:paddingTop="@dimen/padding_3"
                            android:paddingRight="@dimen/padding_10"
                            android:paddingBottom="@dimen/padding_3"
                            android:text="10"
                            android:textAlignment="center"
                            android:textSize="@dimen/text_size_18"
                            app:layout_constraintLeft_toLeftOf="@+id/limit_control"
                            app:layout_constraintTop_toBottomOf="@+id/limit_control" />

                        <EditText
                            android:id="@+id/from_date"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/ractangular_round_white"
                            android:clickable="false"
                            android:enabled="false"
                            android:focusable="false"
                            android:hint="13/06/2019"
                            android:inputType="date"
                            android:maxLength="10"
                            android:maxLines="1"
                            android:minWidth="@dimen/width_80"
                            android:paddingLeft="@dimen/padding_10"
                            android:paddingTop="@dimen/padding_3"
                            android:paddingRight="@dimen/padding_10"
                            android:paddingBottom="@dimen/padding_3"
                            android:textAlignment="center"
                            android:textSize="@dimen/text_size_18"
                            app:layout_constraintLeft_toLeftOf="@+id/range_divider"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/date_range" />

                        <EditText
                            android:id="@+id/to_date"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_15"
                            android:background="@drawable/ractangular_round_white"
                            android:focusable="false"
                            android:hint="13/06/2019"
                            android:inputType="date"
                            android:maxLength="10"
                            android:maxLines="1"
                            android:minWidth="@dimen/width_80"
                            android:paddingLeft="@dimen/padding_10"
                            android:paddingTop="@dimen/padding_3"
                            android:paddingRight="@dimen/padding_10"
                            android:paddingBottom="@dimen/padding_3"
                            android:textAlignment="center"
                            android:textSize="@dimen/text_size_18"
                            app:layout_constraintLeft_toLeftOf="@+id/range_divider"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/date_range" />

                        <RadioGroup
                            android:id="@+id/limit_control"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintLeft_toRightOf="@+id/limit_divider"
                            app:layout_constraintTop_toTopOf="@+id/data_limit">

                            <RadioButton
                                android:id="@+id/unlimited"
                                style="@style/limit_radio"
                                android:onClick="onRadioUnlimitedButtonClicked"
                                android:text="@string/unlimited"
                                android:textColor="@drawable/bg_radio_selection"
                                tools:ignore="UnusedAttribute" />

                            <RadioButton
                                android:id="@+id/limit_to"
                                style="@style/limit_radio"
                                android:layout_marginTop="@dimen/margin_10"
                                android:onClick="onRadioLimitedButtonClicked"
                                android:text="@string/limit_to"
                                android:textColor="@drawable/bg_radio_selection"
                                tools:ignore="UnusedAttribute" />

                        </RadioGroup>

                        <TextView
                            android:id="@+id/data_limit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/opensans_semibold"
                            android:text="@string/data_limit"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_18"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/data_unit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_5"
                            android:fontFamily="@font/opensans_regular"
                            android:text="MB"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_18"
                            app:layout_constraintBottom_toBottomOf="@+id/range"
                            app:layout_constraintLeft_toRightOf="@+id/range"
                            app:layout_constraintTop_toTopOf="@+id/range" />


                        <TextView
                            android:id="@+id/dataLimitError"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_5"
                            android:fontFamily="@font/opensans_regular"
                            android:text="@string/data_limit_must_be_larger_than_used_data"
                            android:textColor="@color/colorRed"
                            android:textSize="@dimen/text_size_12"
                            android:visibility="gone"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/data_unit" />

                        <TextView
                            android:id="@+id/dataUsageLimited"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:text="@{@string/my_data_plan_data_used_of(Util.humanReadableByteCount(dataLimitModel.getUsedData()), Util.humanReadableByteCount(dataLimitModel.getSharedData()))}"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/text_size_14"
                            android:textStyle="bold"
                            android:visibility="invisible"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/dataLimitError" />

                        <TextView
                            android:id="@+id/text_view_data_limit_warning"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="(Data limit exceeding)"
                            android:textColor="@android:color/holo_red_dark"
                            android:textSize="13sp"
                            android:visibility="@{dataLimitModel.usedData >0? (dataLimitModel.usedData>=dataLimitModel.sharedData-Constant.SELLER_MINIMUM_WARNING_DATA? View.VISIBLE:View.GONE) : View.GONE}"
                            app:layout_constraintEnd_toEndOf="@+id/dataUsageLimited"
                            app:layout_constraintStart_toStartOf="@+id/dataUsageLimited"
                            app:layout_constraintTop_toBottomOf="@+id/dataUsageLimited" />

                        <TextView
                            android:id="@+id/dataUsageUnlimited"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:text="@{@string/my_data_plan_data_used(Util.humanReadableByteCount(dataLimitModel.getUsedData()), dataLimitModel.getNumberOfDay())}"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/text_size_14"
                            android:textStyle="bold"
                            android:visibility="invisible"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/dataLimitError" />

                        <TextView
                            android:id="@+id/date_range"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_15"
                            android:fontFamily="@font/opensans_semibold"
                            android:text="@string/date_range"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_18"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/limit_divider_vertical" />

                        <TextView
                            android:id="@+id/from"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_5"
                            android:clickable="false"
                            android:fontFamily="@font/opensans_regular"
                            android:text="@string/from"
                            android:textColor="@color/deep_grey"
                            android:textSize="@dimen/text_size_16"
                            app:layout_constraintBottom_toBottomOf="@+id/from_date"
                            app:layout_constraintRight_toLeftOf="@+id/from_date"
                            app:layout_constraintTop_toTopOf="@+id/from_date" />

                        <TextView
                            android:id="@+id/to"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/margin_5"
                            android:fontFamily="@font/opensans_regular"
                            android:text="@string/to"
                            android:textColor="@color/deep_grey"
                            android:textSize="@dimen/text_size_16"
                            app:layout_constraintRight_toLeftOf="@+id/to_date"
                            app:layout_constraintTop_toBottomOf="@+id/to_date"
                            app:layout_constraintTop_toTopOf="@+id/to_date" />

                        <TextView
                            android:id="@+id/dateError"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_20"
                            android:layout_marginTop="@dimen/margin_5"
                            android:fontFamily="@font/opensans_regular"
                            android:text="@string/date_expired"
                            android:textColor="@color/colorRed"
                            android:textSize="@dimen/text_size_12"
                            android:visibility="gone"
                            app:layout_constraintStart_toStartOf="@+id/to"
                            app:layout_constraintTop_toBottomOf="@+id/to" />

                        <View
                            android:id="@+id/limit_divider"
                            android:layout_width="@dimen/width_1"
                            android:layout_height="@dimen/height_5"
                            android:background="@android:color/transparent"
                            app:layout_constraintBottom_toBottomOf="@+id/data_limit"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/data_limit" />

                        <View
                            android:id="@+id/limit_divider_vertical"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/width_1"
                            android:layout_marginTop="@dimen/margin_20"
                            android:background="@color/grey_400"
                            app:layout_constraintTop_toBottomOf="@+id/dataUsageLimited" />


                        <View
                            android:id="@+id/range_divider"
                            android:layout_width="@dimen/width_1"
                            android:layout_height="@dimen/height_5"
                            android:background="@android:color/transparent"
                            app:layout_constraintBottom_toBottomOf="@+id/date_range"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/date_range" />

                    </android.support.constraint.ConstraintLayout>

                </android.support.constraint.ConstraintLayout>

            </ScrollView>

        </android.support.constraint.ConstraintLayout>

        <android.support.constraint.ConstraintLayout
            android:id="@+id/data_buy_layout"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/margin_15"
            android:layout_marginTop="@dimen/margin_20"
            android:layout_marginEnd="@dimen/margin_15"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_hint">

            <TextView
                android:id="@+id/empty_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TextView
                android:id="@+id/textview_fixed_network_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_5"
                android:fontFamily="@font/opensans_regular"
                android:gravity="center"
                android:text="@string/fixed_by_the_network"
                android:textColor="@color/deep_grey"
                android:textSize="@dimen/text_size_16"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/textView_sell_price_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/opensans_semibold"
                android:gravity="center"
                android:paddingTop="@dimen/padding_10"
                android:textColor="@color/dark_blue"
                android:textSize="@dimen/text_size_18"
